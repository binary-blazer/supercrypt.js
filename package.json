  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "prettier": "3.2.5",
    "typescript": "^5.4.2",
    "undici-types": "^6.9.0"
  }
}
