{
  "name": "supercrypt.js",
  "version": "1.0.0",
  "description": "ðŸŽ‰ modern numeric-ascii encryption & decryption package",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./index.js": "./dist/cjs/index.js",
    "./index.mjs": "./dist/esm/index.js"
  },
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "publish:ready": "pnpm run lint && pnpm run format && pnpm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": {
    "name": "Jonas Franke <@binary-blazer>",
